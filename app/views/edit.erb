<h1>Edit an Article</h1>
<form method="post" action='/articles/<%= @article.id %>'>
  <input id="hidden" type="hidden" name="_method" value="PATCH">
  <p for="title"> Title: <input id="title" type="text" name="title" value="<%= @article.title %>"></p>
  <p for="content"> Content: <input id="content" type="text" name="content" value="<%= @article.content %>"></p>
  <input type="submit" name="Submit">
  </form>
